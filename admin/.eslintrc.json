{
  "extends": ["next/core-web-vitals", "prettier"],
  "rules": {
    "@next/next/no-css-tags": "off",
    "@typescript-eslint/no-unused-vars": "off",
    "unused-imports/no-unused-imports": "error",
    "unused-imports/no-unused-vars": [
      "warn",
      {
        "vars": "all",
        "varsIgnorePattern": "^_",
        "args": "after-used",
        "argsIgnorePattern": "^_"
      }
    ]
  },
  "plugins": ["unused-imports", "prettier"]
}
